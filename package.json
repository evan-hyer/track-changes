{
  "name": "track-changes",
  "description": "Salesforce CLI plugin to track metadata changes.",
  "version": "0.0.0",
  "author": "Evan Hyer",
  "bin": {
    "track-changes": "./bin/run.js"
  },
  "bugs": "https://github.com/evan-hyer/track-changes/issues",
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "@salesforce/core": "^8.24.0",
    "@salesforce/sf-plugins-core": "^12.2.6"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "@types/sinon": "^21.0.0",
    "c8": "^10.1.3",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "0.3.3",
    "sinon": "^21.0.1",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/evan-hyer/track-changes",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "track-changes",
    "dirname": "track-changes",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " "
  },
  "repository": "evan-hyer/track-changes",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "check:readme": "npx oclif readme && git diff --exit-code README.md",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "c8 mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
